            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_open_in_browser"
            app:ifl_attr_ui_subtitleText="Français"
            app:ifl_attr_ui_titleText="Hem Pal" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_espanol"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_iconRes="@drawable/ifl_flag_es"
            app:ifl_attr_ui_iconResTint="false"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_open_in_browser"
            app:ifl_attr_ui_subtitleText="Español"
