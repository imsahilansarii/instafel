            app:ifl_attr_ui_subIconRes="@drawable/ifl_open_in_browser"
            app:ifl_attr_ui_subtitleText="Deutsch"
            app:ifl_attr_ui_titleText="Rüşan Gür" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_france"
