        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_indian"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_iconRes="@drawable/ifl_flag_hi"
            app:ifl_attr_ui_iconResTint="false"
