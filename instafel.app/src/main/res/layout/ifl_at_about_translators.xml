<?xml version="1.0" encoding="utf-8"?>
<me.mamiiblt.instafel.ui.Page
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="me.mamiiblt.instafel.activity.about.ifl_a_about_translators">

    <me.mamiiblt.instafel.ui.PageTitle
        android:id="@+id/ifl_page_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:ifl_attr_ui_titleText="@string/ifl_a10_02" />

    <me.mamiiblt.instafel.ui.PageContentArea
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ifl_page_title">

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_john"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:ifl_attr_ui_iconResTint="false"
            app:ifl_attr_ui_iconRes="@drawable/ifl_flag_el"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_open_in_browser"
            app:ifl_attr_ui_subtitleText="Ελληνικά"
            app:ifl_attr_ui_titleText="John Xirouchakis" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_munir"
            android:layout_width="match_parent"
