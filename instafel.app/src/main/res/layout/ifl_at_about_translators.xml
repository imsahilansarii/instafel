            app:ifl_attr_ui_iconRes="@drawable/ifl_flag_hu"
            app:ifl_attr_ui_spaceBottom="visible"
            app:ifl_attr_ui_enableSubIcon="visible"
            app:ifl_attr_ui_subIconRes="@drawable/ifl_open_in_browser"
            app:ifl_attr_ui_subtitleText="Magyar"
            app:ifl_attr_ui_titleText="Zan" />

        <me.mamiiblt.instafel.ui.TileLarge
            android:id="@+id/ifl_tile_lang_deutch"
            android:layout_width="match_parent"
